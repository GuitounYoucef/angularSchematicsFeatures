{
    "$schema": "http://json-schema.org/schema",
    "$id": "ui-feature",
    "title": "UI library Component Schema",
    "type": "object",
    "properties": {
      "name": {
        "description": "The name of the ui feature",
        "type": "string"
      },
      "path": {
        "description": "Where should be created the file",
        "type": "string",
